<% provide(:title, "Log in") %>
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>Login</h1>
    <%= form_tag '/login' do %>
      Email: <%= text_field_tag :username %>
      Password: <%= password_field_tag :password %>
    <%= submit_tag "Submit" %>
    <% end %>
  </div>
</div>
