<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Property type</th>
      <th>Property values</th>
      <th>In development</th>
      <th>Development</th>
      <th>City</th>
      <th>Province</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.team_id %></td>
        <td><%= property.property_type %></td>
        <td><%= property.property_values_id %></td>
        <td><%= property.in_development %></td>
        <td><%= property.development_id %></td>
        <td><%= property.city %></td>
        <td><%= property.province %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
